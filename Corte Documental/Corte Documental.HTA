		<div class="tabela-celula">
            <div id="CarregarDepositosCadastrados"></div>
        </div>

</div>

<!-- Página Entradas e Saídas -->
<div id="PaginaEstoque" style="display: none;">
<button onclick="AlternarPagina 'PaginaInicial'">⬅ Voltar pagina inicial</button>
<button onclick="AtualizarEstoques">Atualizar Estoques</button><br>
<H1>PAGINA ESTOQUE - CORTE DOCUMENTAL</H1>

		<div class="tabela-celula">
            <div id="CarregarEstoqueDepositos"></div>
        </div>

</div>

<!-- Página Entradas e Saídas -->
<div id="PaginaProgramaçao" style="display: none;">
